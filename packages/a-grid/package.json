{
  "name": "@ayingott/a-grid",
  "type": "module",
  "version": "0.0.2",
  "author": "LoTwT <l709937065@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/LoTwT/a-grid#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LoTwT/a-grid.git"
  },
  "bugs": {
    "url": "https://github.com/LoTwT/a-grid/issues"
  },
  "keywords": [
    "grid",
    "css grid",
    "grid layout",
    "a-grid"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./resolver": {
      "import": "./dist/resolver.js",
      "require": "./dist/resolver.cjs"
    },
    "./global": {
      "types": "./global.d.ts"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist",
    "global.d.ts"
  ],
  "scripts": {
    "dev": "pnpm run build:resolver && vite build -w",
    "build": "pnpm run build:resolver && pnpm run build:components",
    "build:components": "vite build",
    "build:resolver": "tsup"
  },
  "dependencies": {
    "magic-string": "^0.30.11",
    "vue": "^3.5.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.1.3",
    "sass": "^1.77.8",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4",
    "unplugin-auto-import": "^0.18.2",
    "unplugin-vue-components": "^0.27.4",
    "vite": "^5.4.2",
    "vite-plugin-dts": "^4.1.0"
  }
}
